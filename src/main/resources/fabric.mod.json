{
  "schemaVersion": 1,
  "id": "omine",
  "version": "$version",
  "name": "$name",
  "description": "$description",
  "authors": [
    "vini2003"
  ],
  "contact": {
    "email": "contact@vini2003.dev"
  },
  "license": "MIT",
  "icon": "assets/omine/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.omine.Omine"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.omine.OmineClient"
      }
    ],
    "cardinal-components": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.omine.registry.common.HarmfulGasComponents"
      }
    ]
  },
  "accessWidener": "mod.accesswidener",
