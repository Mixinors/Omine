{
  "schemaVersion": 1,
  "id": "countdown",
  "version": "${version}",
  "name": "Countdown",
  "description": "Adds the big countdown to the SMPHardcore world.",
  "authors": [
    "vini2003"
  ],
  "contact": {
    "email": "contact@vini2003.dev"
  },
  "license": "MIT",
  "icon": "assets/countdown/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.bodyshufflelite.BSL"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.bodyshufflelite.BSLClient"
      }
    ],
    "cardinal-components": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.bodyshufflelite.registry.common.BSLComponents"
      }
    ]
  },
  "accessWidener": "bodyshufflelite.accesswidener",
  "mixins": [
    "bodyshufflelite.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "fabric": ">=0.35.1+1.17",
    "minecraft": "1.17"
  },
  "custom": {
    "cardinal-components": [
      "bodyshufflelite:body_parts"
    ]
  }
}
