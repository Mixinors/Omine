{
  "schemaVersion": 1,
  "id": "skinshuffle",
  "version": "${version}",
  "name": "Skin Shuffle",
  "description": "Adds a skin-changing command to Minecraft.",
  "authors": [
    "vini2003"
  ],
  "contact": {
    "email": "contact@vini2003.dev"
  },
  "license": "MIT",
  "icon": "assets/skinshuffle/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.skinshuffle.SS"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.skinshuffle.SSClient"
      }
    ],
    "cardinal-components": [
      {
        "adapter": "kotlin",
        "value": "dev.vini2003.skinshuffle.registry.common.SSComponents"
      }
    ]
  },
  "accessWidener": "skinshuffle.accesswidener",
  "mixins": [
    "skinshuffle.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "fabric": ">=0.35.1+1.17",
    "minecraft": "1.17.1"
  },
  "custom": {
    "cardinal-components": [
      "skinshuffle:skin"
    ]
  }
}
